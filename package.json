    "@calm-ui/radio-group": "workspace:^",
    "@calm-ui/ripple": "workspace:^",
    "@calm-ui/select": "workspace:^",
    "@calm-ui/switch": "workspace:^",
    "@calm-ui/theme": "workspace:^",
    "@calm-ui/tooltip": "workspace:^"
  },
  "peerDependencies": {
    "react": "19.x"
  },
